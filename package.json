{
	"name": "@tammergard/eslint-config-base",
	"version": "4.0.0",
	"description": "A sharable ESLint config for JavaScript with personal preferences",
	"scripts": {
		"format": "prettier --write .",
		"format-check": "prettier --check .",
		"lint": "eslint .",
		"release": "pnpm changeset publish",
		"review": "pnpm install --frozen-lockfile && pnpm format-check && pnpm lint"
	},
	"dependencies": {
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-import": "2.27.5",
		"eslint-plugin-prettier": "4.2.1"
	},
	"devDependencies": {
		"@changesets/cli": "2.26.1",
		"@tammergard/prettier-config": "2.0.0",
		"prettier": "2.8.7"
	},
	"peerDependencies": {
		"eslint": ">=8.37.0",
		"prettier": ">=2.8.7"
	},
	"files": [
		"index.js"
	],
	"main": "index.js",
	"keywords": [
		"eslint",
		"eslint-config",
		"config",
		"style-guide",
		"javascript"
	],
	"homepage": "https://github.com/filiptammergard/eslint-config-base#readme",
